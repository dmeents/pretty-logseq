{
  "name": "pretty-logseq",
  "version": "1.0.3",
  "description": "A Logseq plugin for custom page preview popovers with rich property display",
  "main": "dist/index.html",
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "check": "biome check --write .",
    "check:ci": "biome check ."
  },
  "logseq": {
    "id": "pretty-logseq",
    "title": "Pretty Logseq",
    "icon": "./assets/logos/pretty-logseq-logo.png"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@logseq/libs": "^0.0.17",
    "@testing-library/dom": "^10.4.1",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "jsdom": "^28.0.0",
    "sass": "^1.97.3",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-logseq": "^1.1.2",
    "vitest": "^4.0.18"
  },
  "keywords": [
    "logseq",
    "plugin",
    "popover",
    "preview",
    "ui"
  ],
  "license": "MIT",
  "packageManager": "yarn@4.12.0"
}
