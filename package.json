{
  "name": "pretty-logseq",
  "version": "0.1.0",
  "description": "A Logseq plugin for custom page preview popovers with rich property display",
  "main": "dist/index.html",
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "format": "biome format --write .",
    "check": "biome check --write ."
  },
  "logseq": {
    "id": "pretty-logseq",
    "title": "Pretty Logseq"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@logseq/libs": "^0.0.17",
    "sass": "^1.97.3",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vite-plugin-logseq": "^1.1.2"
  },
  "keywords": [
    "logseq",
    "plugin",
    "popover",
    "preview",
    "ui"
  ],
  "license": "MIT",
  "packageManager": "yarn@4.12.0"
}
